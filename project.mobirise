es\" class=\"carousel-inner \">\n                        <div class=\"carousel-item text-left\">\n                            <div class=\"user \">\n                                <p class=\"user_text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".user_text, .author\">Lorem ipsum dolor sit amet, consectetur adipisicing elit. Vitae nostrum, quos voluptas fugiat blanditiis, temporibus expedita cumque doloribus!</p>\n                                <div class=\"author\" mbr-if=\"showAuthor\">\n                                <div class=\"user_image\">\n                                    <img src=\"../_images/faces/face1.jpg\" alt title>\n                                </div>\n                                    <div class=\"author__info\">\n                                <div mbr-text class=\"user_name mbr-medium mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".user_name\">\n                                    John\n                                </div>\n                                <div mbr-text class=\"user_prof mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".user_prof\">\n                                    Project Manager\n                                </div>\n                                    </div>\n                                </div>\n                            </div>\n                        </div>\n                        <div class=\"carousel-item text-left\">\n                            <div class=\"user\">\n                                <p class=\"user_text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".user_text, .author\">Lorem ipsum dolor sit amet, consectetur adipisicing elit. Vitae nostrum, quos voluptas fugiat blanditiis, temporibus expedita cumque doloribus!</p>\n                                <div class=\"author\" mbr-if=\"showAuthor\">\n                                <div class=\"user_image\">\n                                    <img src=\"../