=\"bgBlock\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax=\"\">\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/background/backgraund__trend.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#06df9e\" selected=\"\">\n            <input type=\"video\" title=\"Background Video\" value=\"https://youtu.be/iM-NestDPNQ\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Gradient Background\" name=\"gradientBg\" condition=\"bg.type=='color'\" checked=\"\">\n        <input type=\"color\" title=\"Color 2\" name=\"color2\" value=\"#06df9e\" condition=\"gradientBg &amp;&amp;bg.type=='color'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#5eeebb\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"checkbox\" title=\"Gradient Overlay\" name=\"gradientOverlay\" checked=\"\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color 2\" name=\"overlayColor2\" value=\"#51aafa\" condition=\"gradientOverlay &amp;&amp; overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.7\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity}\">\n    </div>\n    <div class=\"animation__background\" mbr-if=\"showAnimation\">\n        <img src=\"../_images/faces/face14.jpg\" alt=\"\" class=\"image\" title=\"\">\n        <img src=\"../_images/faces/face2.jpg\" alt=\"\" class=\"image\" title=\"\">\n        <img src=\"../_images/faces/face9.jpg\" alt=\"\" class=\"image\" title=\"\">\n        <img src=\"../_images/faces/face11.jpg\" alt=\"\" class=\"image\" title=\"\">\n        <img src=\"../_images/faces/face4.jpg\" alt=\"\" class=\"image\" title=\"\">\n        <img src=\"../_images/faces/face7.jpg\" alt=\"\" class=\"image\" title=\"\">\n        <div class=\"circle\"></div>\n        <div class=\"circle2\"></div>\n    </div>\n\n    <div class=\"container\">\n        <div class=\"row justify-content-center\">\n            <div class=\"title__block col-lg-9\">\n                <div class=\"icon__wrap\" mbr-if=\"showIcon\">\n                    <div class=\"icon__block\">\n                        <span mbr-icon=\"\" class=\"mbr-iconfont mobi-mbri-quote-left mobi-mbri\"></span>\n                    </div>\n                </div>\n               <h2 class=\"mbr-section-title mbr-fonts-style\" mbr-if=\"showTitle\" mbr-theme-style=\"display-2\" data-app-selector=\".mbr-section-title, .icon__wrap\">What do People Say <br> About us.</h2>\n              <h3 class=\"mbr-section-subtitle mbr-regular mbr-fonts-style\" mbr-if=\"showSubtitle\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-section-subtitle\">Lorem ipsum dolor sit amet, consectetur adipisicing elit. <br>Odit sed temporibus voluptatum minus ipsa blanditiis doloremque odio.</h3>\n            </div>\n              \n        <div class=\"col-lg-8 margin__bot\">\n            <div class=\"wrap\">\n                <div class=\"icon__bg\"></div>\n                    <div class=\"carousel slide\" role=\"listbox\" data-pause=\"true\" data-keyboard=\"false\" data-ride=\"{{showAutoplay ? 'carousel' : 'false'}}\" data-interval=\"{{showAutoplay ? interval : 'false'}}\">\n                <div mbr-list=\"\" mbr-list-grow=\"testimonialsSlides\" class=\"carousel-inner\">\n                    <div class=\"carousel-item\">\n                        <div class=\"carousel__block\">\n                            <p class=\"carousel__text mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".carousel__text\">Lorem ipsum dolor sit amet, consectetur adipisicing elit. Vitae nostrum, quos voluptas fugiat blanditiis, temporibus expedita cumque doloribus!</p>\n                            <div class=\"author\" mbr-if=\"showName || showProf\">\n                            <div mbr-text=\"\" class=\"user_name mbr-medium mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showName\" data-app-selector=\".user_name\">\n                                ALEX\n                            </div>\n                            <div mbr-text=\"\" class=\"user_prof mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showProf\" data-app-selector=\".user_prof\">\n                                Project Manager\n                            </div>\n                                </div>\n                        </div>\n                    </div>\n                    <div class=\"carousel-item\">\n                        <div class=\"carousel__block\">\n                            <p class=\"carousel__text mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".carousel__text\">Lorem ipsum dolor sit amet, consectetur adipisicing elit. Vitae nostrum, quos voluptas fugiat blanditiis, temporibus expedita cumque doloribus!</p>\n                            <div class=\"author\" mbr-if=\"showName || showProf\">\n                            <div mbr-text=\"\" class=\"user_name mbr-medium mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showName\" data-app-selector=\".user_name\">\n                                LINDA\n                            </div>\n                            <div mbr-text=\"\" class=\"user_prof mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showProf\" data-app-selector=\".user_prof\">\n                                Co-founder\n                            </div>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n  