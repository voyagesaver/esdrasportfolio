              }
            },
            ".animation__background": {
              ".triangle": {
                "position": "absolute",
                "right": "10%",
                "top": "50%",
                "animation": "rotated 30s infinite linear",
                "width": "50px",
                "height": "50px"
              },
              ".circle": {
                "width": "54px",
                "height": "54px",
                "border": "10px solid @dangerColor",
